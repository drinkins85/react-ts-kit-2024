{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "jsx": "react" ,
    "module": "NodeNext",
    "pretty": true ,
    "noImplicitAny": false ,
    "skipLibCheck": true ,
    "allowUnreachableCode": false,
    "traceResolution": false,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
